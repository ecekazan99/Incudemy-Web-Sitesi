@model Angle.Models.PartnersModel
@{
    ViewBag.Title = "Partners";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section  id="content">
    <div  class="container" >
        <div class="row">
            <div style="margin:auto 0;" class="col-lg-12">

                @foreach (var item in Model.Allpartners)
                {
                    
                    
                <a class="hover-wrap fancybox" data-fancybox-group="gallery" title="@item.Title" href="@item.FileWay">
                    <span class="overlay-img"></span>
                    <span class="overlay-img-thumb font-icon-plus"></span>
                </a>

                    <img style="margin:20px; max-width:35%;height:35%;" src="@Url.Content(item.File)" alt="@item.Description" class="pull-left"  />

                    
                }


            </div>
        </div>
    </div>
</section>